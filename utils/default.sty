\renewcommand{\!}{\hspace*{0em}}
\renewcommand{\-}{\vspace*{-0.5em}}
\renewcommand{\_}{\vspace*{-1em}}
\renewcommand{\;}{\vspace*{0.5em}}
\renewcommand{\:}{\vspace*{1em}}
\renewcommand{\|}{ $|$ }
\renewcommand{\thesection}{\arabic{section}}
\newcommand{\acenter[2]}{\begin{center}#2\end{center}}
\newcommand{\aleft[2]}{\begin{flushleft}#2\end{flushleft}}
\newcommand{\aright[2]}{\begin{flushright}#2\end{flushright}}
\newcommand{\up[2]}{\ensuremath{\mathrm{#2}}}
\newcommand{\hidesection[2]}{\phantomsection\addcontentsline{toc}{section}{#2}
    \markboth{}{#2}\section*{#2}}
\newcommand{\class}{}
\newcommand{\name}{}
\newcommand{\subtitle}{}
\newcolumntype{L}[1]{>{\raggedright\arraybackslash}p{#1}}
\newcolumntype{C}[1]{>{\centering\arraybackslash}p{#1}}
\newcolumntype{R}[1]{>{\raggedleft\arraybackslash}p{#1}}
\newcolumntype{Y}{>{\centering\arraybackslash}X}
\newcolumntype{Z}{>{\raggedleft\arraybackslash}X}
\newenvironment{block}{\vspace*{-0.5\baselineskip}\list{}{\leftmargin=0em\rightmargin=0em %
    \itemsep=0.5\baselineskip\parsep=0em\item\relax}}{\endlist\vspace*{0.75\baselineskip}}
\newenvironment{subblock}{\vspace*{-0.5\baselineskip}\list{}{\leftmargin=1em\rightmargin=1em %
    \itemsep=0.5\baselineskip\parsep=0em\item\relax}}{\endlist\vspace*{0.75\baselineskip}}
\newenvironment{parblock}{\vspace*{-\baselineskip}\list{}{\leftmargin=0em %
    \rightmargin=0em\itemindent=\parindent\itemsep=0em\parsep=0em} %
    \item\relax}{\endlist\vspace*{0.25\baselineskip}}

