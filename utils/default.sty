\renewcommand{\maketitle}{\begin{titlepage}\vspace*{\fill}\vspace*{-6em} %
    \acenter{\LARGE~\title~\\[0.1em]\large~\subtitle~\\[3em]\small~\name~\\ %
    ~\class~\\~\date~}\vspace*{\fill}\end{titlepage}}
\newcommand{\makesmalltitle}{\thispagestyle{empty}\vspace*{-6em}\aright{\small %
    ~\name~\\~\date~\\~\class~}\acenter{\large~\title~\\\normalsize~\subtitle~}}
\newcommand{\makeheader}{\pagestyle{fancy}\lhead{\footnotesize\title} %
    \rhead{\footnotesize\!Page~\thepage~of~\pageref{LastPage}}\cfoot{}}
\newcommand{\makesecheader}{\pagestyle{fancy}\lhead{\footnotesize\nouppercase{\rightmark}} %
    \rhead{\footnotesize\!Page~\thepage~of~\pageref{LastPage}}\cfoot{} %
    \renewcommand{\sectionmark}[1]{\markright{\thesection\hspace*{1em}##1}}}
\newcommand{\maketoc}{\setlength{\cftbeforetoctitleskip}{0em} %
    \setlength{\cftaftertoctitleskip}{1em}\newpage %
    \tableofcontents\thispagestyle{empty}\vspace*{\fill}\newpage}
\newcommand{\makeref}{\newpage\thispagestyle{empty}\phantomsection\markboth{}{References} %
    \nocite{*}\printbibliography[heading=subbibintoc,title={References}]\newpage}
